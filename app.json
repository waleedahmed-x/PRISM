{
  "expo": {
    "name": "PRISM",
    "slug": "prism",
    "version": "1.0.0",
    "orientation": "portrait",
    "platforms": ["ios", "android"],
    "icon": "./assets/splash/splash.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "gg.gamegpt.prism",
      "buildNumber": "1",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "This app needs access to your photo library to upload your profile picture.",
        "NSCameraUsageDescription": "This app needs access to your camera to take profile pictures."
      },
      "appleTeamId": "GGU5VHWX77"
    },
    "android": {
      "package": "gg.gamegpt.prism",
      "adaptiveIcon": {
        "foregroundImage": "./assets/splash/splash.png",
        "backgroundColor": "#ffffff"
      }
    },
    "scheme": "gg.gamegpt.prism",
    "web": {
      "favicon": "./assets/splash/splash.png"
    },
    "plugins": ["expo-secure-store"],
    "extra": {
      "eas": {
        "projectId": "92856fda-b6b7-4cf8-af9b-9b334b73a105"
      }
    },
    "owner": "Haider Sattar"
  }
}
