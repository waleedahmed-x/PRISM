{
  "name": "@privy-io/api-base",
  "version": "1.2.3",
  "engines": {
    "npm": ">=8.0.0",
    "node": ">=18.0.0"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typings": "./dist/index",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./constants": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./constants/*": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./classes": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "npx tsup --clean --minify",
    "clean": "rm -rf dist .turbo",
    "dev": "npx tsup --watch",
    "lint": "eslint ./**/*.ts && npx tsc --noEmit",
    "format": "eslint ./**/*.ts --fix",
    "test": "jest --testMatch \"**/test/**/*.test.ts\"",
    "test:ci": "npm run test"
  },
  "dependencies": {
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@privy-io/tsconfig": "*",
    "@privy-io/eslint-config-custom": "*",
    "tsup": "^6.2.3",
    "typescript": "~5.5.3"
  },
  "files": [
    "dist/**/*",
    "LICENSE",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  }
}
