{
  "name": "@privy-io/public-api",
  "version": "2.9.4",
  "engines": {
    "npm": ">=8.0.0",
    "node": ">=18.0.0"
  },
  "license": "Apache-2.0",
  "source": "./index.ts",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typings": "./dist/index",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./schemas": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./schemas/*": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "targets": {
    "main": {
      "isLibrary": true,
      "sourceMap": false
    },
    "module": {
      "isLibrary": true,
      "sourceMap": false
    }
  },
  "scripts": {
    "build": "npx parcel build --no-cache",
    "clean": "rm -rf dist .turbo",
    "dev": "npx parcel watch -p 2234",
    "lint": "eslint ./**/*.ts && npx tsc --noEmit",
    "format": "eslint ./**/*.ts --fix",
    "test": "jest --testMatch \"**/test/**/*.test.ts\"",
    "test:ci": "npm run test"
  },
  "dependencies": {
    "@privy-io/api-base": "1.2.3",
    "bs58": "^5.0.0",
    "ethers": "^5.7.2",
    "libphonenumber-js": "^1.10.31",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@privy-io/eslint-config-custom": "*",
    "@privy-io/parcel-config": "*",
    "@privy-io/tsconfig": "*",
    "typescript": "~5.5.3"
  },
  "files": [
    "dist/**/*",
    "LICENSE",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  }
}
